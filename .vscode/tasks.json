{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: clang++ 生成活动文件",
            "command": "/usr/bin/clang++",
            "args": [
                "-fcolor-diagnostics",
                "-fansi-escape-codes",
                "-I/opt/homebrew/include",
                "-I/opt/homebrew/opt/eigen/include/eigen3",
                "-std=c++17",
                "-L/opt/homebrew/lib",
                "-lboost_iostreams",
                "-o",
                "test",
                "HydrogenAtom.cpp"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "调试器生成的任务。"
        }
    ],
    "version": "2.0.0"
}